<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <UI>
            <Dialog Id="ProgramSettingsDlg" Width="370" Height="270" Title="[ProductName] Setup" NoMinimize="yes">
                <Control Id="AnalyticsLabel" Type="Text" X="20" Y="73" Width="290" Height="15" Text="!(loc.ProgramSettingsDlgAnalyticsLabel)" />
                <Control Id="AnalyticsLabel2" Type="Text" X="20" Y="88" Width="290" Height="15" Text="!(loc.ProgramSettingsDlgAnalyticsLabel2)" />
                <Control Id="EnableAnalyticsCheckBox" Type="CheckBox" X="30" Y="103" Width="285" Height="17" Property="ENABLEANALYTICS" CheckBoxValue="1" Text="!(loc.ProgramSettingsDlgAnalyticsCheckbox)" />
                <Control Id="CrashReportingLabel" Type="Text" X="20" Y="125" Width="290" Height="15" Text="!(loc.ProgramSettingsDlgCrashReportsLabel)" />
                <Control Id="CrashReportingLabel2" Type="Text" X="20" Y="140" Width="290" Height="15" Text="!(loc.ProgramSettingsDlgCrashReportsLabel2)" />
                <Control Id="EnableCrashReportingCheckBox" Type="CheckBox" X="30" Y="155" Width="285" Height="17" Property="ENABLECRASHREPORTING" CheckBoxValue="1" Text="!(loc.ProgramSettingsDlgCrashReportsCheckbox)" />
                <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="&amp;Back">
                    <Publish Event="NewDialog" Value="ShortcutSelectionDlg">1</Publish>
                </Control>
                <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="&amp;Next">
                    <Publish Event="NewDialog" Value="VerifyReadyDlg">1</Publish>
                </Control>
                <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="Cancel">
                    <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="WixUI_Bmp_Banner" />
                <Control Id="Description" Type="Text" X="25" Y="23" Width="340" Height="15" Transparent="yes" NoPrefix="yes">
                    <Text>{\MyWixUI_Font_Description}!(loc.ProgramSettingsDlgDescription)</Text>
                </Control>
                <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
                <Control Id="Title" Type="Text" X="15" Y="6" Width="340" Height="15" Transparent="yes" NoPrefix="yes">
                    <Text>{\MyWixUI_Font_Title}!(loc.ProgramSettingsDlgTitle)</Text>
                </Control>
                <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            </Dialog>
        </UI>
    </Fragment>
</Wix>