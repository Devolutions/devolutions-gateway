<?xml version='1.0' encoding='windows-1252'?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <UI>
            <Dialog Id="ShortcutSelectionDlg" Width="370" Height="270" Title="[ProductName] Setup" NoMinimize="yes">
                <Control Id="NameLabel" Type="Text" X="20" Y="73" Width="350" Height="15" Text="!(loc.ShortcutSelectionDlgLabel)" />
                <Control Id="DesktopShortcutCheckBox" Type="CheckBox" X="45" Y="102" Width="305" Height="17" Property="INSTALLDESKTOPSHORTCUT" CheckBoxValue="1" Text="!(loc.DesktopShortcutChoice)" />
                <Control Id="StartMenuShortcutCheckBox" Type="CheckBox" X="45" Y="122" Width="305" Height="17" Property="INSTALLSTARTMENUSHORTCUT" CheckBoxValue="1" Text="!(loc.StartMenuShortcutChoice)" />
                
                <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="&amp;Back">
                  <!--Dlg shown when back is pressed are defined in WaykNow.wxs because it changes depending of the UI used-->
                </Control>
                <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="&amp;Next">
                    <Publish Event="NewDialog" Value="ProgramSettingsDlg">1</Publish>
                </Control>
                <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="Cancel">
                    <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="WixUI_Bmp_Banner" />
                <Control Id="Description" Type="Text" X="25" Y="23" Width="340" Height="15" Transparent="yes" NoPrefix="yes">
                    <Text>{\MyWixUI_Font_Description}!(loc.ShortcutSelectionDlgDescription) </Text>
                </Control>
                <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />
                <Control Id="Title" Type="Text" X="15" Y="6" Width="340" Height="15" Transparent="yes" NoPrefix="yes">
                    <Text>{\MyWixUI_Font_Title}!(loc.ShortcutSelectionDlgTitle)</Text>
                </Control>
                <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
            </Dialog>
        </UI>
    </Fragment>
</Wix>